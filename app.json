{
	"expo": {
		"name": "catevika-mobile-movies-app",
		"slug": "catevika-mobile-movies-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/logo.png",
		"scheme": "movies",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/logo.png",
				"backgroundColor": "#030014"
			},
			"package": "com.catevika.movieSearch"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/logo.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/logo.png",
					"imageWidth": 200,
					"resizeMode": "contain"
				}
			],
			"expo-secure-store",
			[
				"expo-font",
				{
					"fonts": [
						"./node_modules/@expo-google-fonts/barlow/Barlow_400Regular.ttf"
					]
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "cd37fdb5-63c9-47e7-bafc-71d9ece3bdff"
			}
		},
		"owner": "catevika"
	}
}
